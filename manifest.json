{
    "manifest_version": 2,
    "name": "medusa",
    "description": "This extension allows treating multiple branches in github as a main one",
    "version": "0.0.1",
    "permissions": [
	"storage",
	"https://github.com/*"
    ],
    "content_scripts": [{
	"matches": ["https://github.com/*"],
	"js": ["js/contentscript.js"]
    }],
    "options_page": "options.html"
}
